@model auth.Models.AccountViewModels.LoggedOutViewModel

<div class="page-header">
    <h1>
		<small>You have successfully logged out of the application.</small>
    </h1>

    @if (Model.PostLogoutRedirectUri != null)
    {
	<div>
		Return to <a href="@Model.PostLogoutRedirectUri">@Model.ClientName</a>
	</div>
    }

    @if (Model.SignOutIframeUrl != null)
    {
        <iframe style="display:none" width="0" height="0" class="signout" src="@Model.SignOutIframeUrl"></iframe>
    }
</div>
